Class butt{

private String malachi, aidan, zac;


public butt(String malachi, String aidan, String zac){
this.malachi = malachi;
this.aiden = aidan;
this.zac = zac;
}

public String getMalachi(){
return malachi;
}

public String getAidan(){
return aidan;
}

public String getZac(){
return zac;
}

}

Class poop{
public static void main (String args[]){
butt daBois = new butt("poop","poopier","poopiest");

System.out.println("Malachi is a: " + daBois.getMalachi() +", while Aidan is a: " + daBois.getAidan()
+". Lastly Zac is the: " + daBois.getZac());
}
}

//Was worth it ;)
