class Butt {
    private String malachi, aidan, zac, rina;

    public Butt(String malachi, String aidan, String zac, String rina){
        this.malachi = malachi;
        this.aidan = aidan;
        this.zac = zac;
        this.rina = rina;
    }
    
    public String getMalachi(){
        return malachi;
    }

    public String getAidan(){
        return aidan;
    }

    public String getZac(){
        return zac;
    }
    
    public String getRina(){
        return rina;
    }
}

class Poop{
    public static void main (String args[]){
    Butt daBois = new Butt("poop","poopier","poopiest","poopiest x2");

    System.out.println("Malachi is a: " + daBois.getMalachi() +", while Aidan is a: "+daBois.getAidan()
    +", Zac is the: "+daBois.getZac() + ",\nand Rina is the: " + daBois.getRina());
    }
}

//Was worth it ;)
